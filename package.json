{
  "name": "react-native-cache-store",
  "version": "1.0.0",
  "description": "A localStorage-like wrapper around React Native's AsyncStorage with cache expiration.",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "node_modules/react-native/jestSupport/preprocessor.js",
    "setupEnvScriptFile": "node_modules/react-native/jestSupport/env.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "packager/react-packager/src/Activity/"
    ],
    "testFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "promise",
      "source-map"
    ]
  },
  "keywords": [
    "react-native",
    "react",
    "cache",
    "lscache",
    "asyncstorage",
    "storage"
  ],
  "author": "Lim Chee Aun",
  "homepage": "https://github.com/cheeaun/react-native-cache-store",
  "bugs": {
    "url": "https://github.com/cheeaun/react-native-cache-store/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cheeaun/react-native-cache-store.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "~10.0.1",
    "babel-polyfill": "~6.7.4",
    "babel-preset-es2015": "~6.6.0",
    "jest-cli": "~0.10.0",
    "react": "~0.14.8",
    "react-native": "~0.22.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "jest-hoist"
    ]
  }
}
